using System;
using System.Collections.Generic;

class Program {
    static void Main(string[] args) {
        List<char> qo = new List<char> {'O', 'Q', 'Q', 'Q', 'Q', 'Q', 'Q'};
        for(int i = 0; i < qo.Count; i++)
        {
          if(qo[i] == 'O')
          {
            Console.WriteLine(i);
          }
        }
    }
}
